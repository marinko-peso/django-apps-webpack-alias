  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marinko-peso/django-apps-webpack-alias.git"
  },
  "keywords": [
    "django",
    "apps",
    "webpack",
    "alias",
    "auto",
    "discovery"
  ],
  "author": "Marinko Peso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marinko-peso/django-apps-webpack-alias/issues"
  },
  "homepage": "https://github.com/marinko-peso/django-apps-webpack-alias#readme",
  "devDependencies": {
    "bili": "^3.1.2",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.1.0"
